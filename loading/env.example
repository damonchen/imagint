# Environment Variables Configuration
# Copy this file to .env.local and update the values according to your setup

# API Configuration
NEXT_PUBLIC_API_URL=http://192.168.187.144/:3000

# Authentication
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id_here
NEXT_PUBLIC_GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Database (if needed)
DATABASE_URL=your_database_url_here

# JWT Secret (for server-side)
JWT_SECRET=your_jwt_secret_here

# Environment
NODE_ENV=development

# Optional: Analytics
NEXT_PUBLIC_GA_TRACKING_ID=your_ga_tracking_id_here

# Optional: Feature flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_DEBUG_MODE=true 